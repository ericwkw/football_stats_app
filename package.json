{
  "name": "football_stats_app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev --experimental-allow-node-fallback",
    "dev:turbo": "next dev --turbopack --experimental-allow-node-fallback",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "create-exec-sql": "psql $(grep DATABASE_URL .env.local | cut -d '=' -f2) -f scripts/create_exec_sql_function.sql",
    "update-schema": "node scripts/run-sql.js scripts/update_schema.sql",
    "update-schema-js": "node scripts/update-schema.js",
    "add-mock-data": "psql -f sql/debug/anonymized_mock_data.sql $DATABASE_URL",
    "fix-team-impact": "psql -f sql/fixes/fix_team_impact_function.sql $DATABASE_URL",
    "test": "jest",
    "import-data": "ts-node src/scripts/import-historical-data.ts",
    "copy-templates": "node scripts/copy-templates.js",
    "prebuild": "npm run copy-templates"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.7",
    "@vercel/analytics": "^1.5.0",
    "autoprefixer": "^10.4.19",
    "chart.js": "^4.4.9",
    "csv-parse": "^5.5.5",
    "dotenv": "^16.5.0",
    "lucide-react": "^0.511.0",
    "next": "15.3.2",
    "postcss": "^8.4.38",
    "react": "^19.0.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.0.0",
    "react-toastify": "^11.0.5",
    "recharts": "^2.15.3",
    "tailwindcss": "^3.4.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@nodelib/fs.scandir": "^4.0.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "resolutions": {
    "lightningcss": "1.22.0"
  }
}
